{
    "name": "brianium/paratest",
    "type": "library",
    "description": "Parallel testing for PHP",
    "keywords": [
        "testing",
        "PHPUnit",
        "concurrent",
        "parallel"
    ],
    "homepage": "https://github.com/paratestphp/paratest",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Scaturro",
            "email": "scaturrob@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "phpunit/php-code-coverage": "10.0.x-dev",
        "phpunit/php-file-iterator": "4.0.x-dev",
        "phpunit/php-text-template": "3.0.x-dev",
        "phpunit/php-timer": "6.0.x-dev",
        "phpunit/phpunit": "10.0.x-dev",
        "sebastian/code-unit-reverse-lookup": "3.0.x-dev",
        "sebastian/diff": "^5.0.x-dev",
        "sebastian/environment": "^6.0.x-dev",
        "sebastian/global-state": "^6.0.x-dev",
        "sebastian/object-enumerator": "^5.0.x-dev",
        "sebastian/object-reflector": "^3.0.x-dev",
        "sebastian/recursion-context": "^5.0.x-dev",
        "symfony/console": "^5.3.0",
        "symfony/process": "^5.3.0"
    },
    "require-dev": {
        "ext-posix": "*",
        "doctrine/coding-standard": "^9.0.0",
        "ekino/phpstan-banned-code": "^0.4.0",
        "ergebnis/phpstan-rules": "^0.15.3",
        "phpstan/phpstan": "^0.12.88",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-phpunit": "^0.12.19",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpunit/php-invoker": "4.0.x-dev",
        "sebastian/cli-parser": "2.0.x-dev",
        "sebastian/comparator": "5.0.x-dev",
        "sebastian/complexity": "3.0.x-dev",
        "sebastian/code-unit": "2.0.x-dev",
        "sebastian/exporter": "5.0.x-dev",
        "sebastian/lines-of-code": "2.0.x-dev",
        "sebastian/type": "3.0.x-dev",
        "sebastian/version": "4.0.x-dev",
        "squizlabs/php_codesniffer": "^3.6.0",
        "symfony/filesystem": "^5.3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist",
            "phpunit/phpunit": "source"
        }
    },
    "autoload": {
        "psr-4": {
            "ParaTest\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ParaTest\\Tests\\": "test/"
        }
    },
    "bin": [
        "bin/paratest"
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/Slamdunk"
        },
        {
            "type": "paypal",
            "url": "https://paypal.me/filippotessarotto"
        }
    ]
}
